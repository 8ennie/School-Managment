<+><p-table\r\n  [columns]=\"cols\"\r\n  [value]=\"monitors\"\r\n  selectionMode=\"single\"\r\n  [(selection)]=\"selectedMonitor\"\r\n  styleClass=\"p-m-3\"\r\n  (onRowSelect)=\"editMonitor($event.data)\"\r\n  [rows]=\"15\"\r\n>\r\n  <ng-template pTemplate=\"caption\">\r\n    <div class=\"p-d-flex p-text-center p-jc-center\">\r\n      <h1 class=\"p-m-0\">{{ \"monitor.list.header\" | translate }}</h1>\r\n      <div class=\"p-ml-auto\">\r\n        <p-button\r\n          class=\"p-ml-auto\"\r\n          label=\"{{ 'monitor.list.reboot-all' | translate }}\"\r\n          (click)=\"restartAll()\"\r\n          styleClass=\"p-button-warning\"\r\n        ></p-button>\r\n        <p-button\r\n          icon=\"pi pi-plus\"\r\n          (click)=\"newMonitor()\"\r\n          class=\"p-ml-2\"\r\n          label=\"{{ 'monitor.list.add' | translate }}\"\r\n        >\r\n        </p-button>\r\n      </div>\r\n    </div>\r\n  </ng-template>\r\n  <ng-template pTemplate=\"header\" let-columns>\r\n    <tr>\r\n      <th *ngFor=\"let col of columns\" [pSortableColumn]=\"col.field\">\r\n        {{ col.header | translate }}\r\n        <p-sortIcon [field]=\"col.field\"></p-sortIcon>\r\n      </th>\r\n    </tr>\r\n  </ng-template>\r\n\r\n  <ng-template pTemplate=\"body\" let-monitor>\r\n    <tr [pSelectableRow]=\"monitor\">\r\n      <td>\r\n        {{ monitor[\"name\"] }}\r\n      </td>\r\n      <td>\r\n        {{ monitor[\"location\"] }}\r\n      </td>\r\n      <td>\r\n        {{\r\n          (monitor[\"active\"]\r\n            ? monitor[\"status\"] != null\r\n              ? monitor[\"status\"] === true\r\n                ? \"monitor.list.on\"\r\n                : \"monitor.list.off\"\r\n              : \"monitor.list.offline\"\r\n            : \"monitor.list.inactive\"\r\n          ) | translate\r\n        }}\r\n      </td>\r\n      <td>\r\n        {{\r\n          (monitor[\"imageShow\"]\r\n            ? monitor[\"imageShow\"].area\r\n            : \"monitor.list.no-area\"\r\n          ) | translate\r\n        }}\r\n      </td>\r\n    </tr>\r\n  </ng-template>\r\n</p-table>\r\n\r\n<p-dialog\r\n  [(visible)]=\"monitorDialog\"\r\n  header=\"{{ monitor?.name + '  ' + ('monitor.details.header' | translate) }}\"\r\n  [modal]=\"true\"\r\n  styleClass=\"p-fluid\"\r\n  [draggable]=\"false\"\r\n>\r\n  <ng-template pTemplate=\"content\">\r\n    <p-tabView styleClass=\"dialog\">\r\n      <p-tabPanel header=\"{{ 'monitor.details.settings' | translate }}\">\r\n        <div class=\"p-fluid\">\r\n          <div class=\"p-field p-grid\">\r\n            <label for=\"name\" class=\"p-col-12 p-mb-2 p-md-2 p-mb-md-0\">\r\n              {{ \"monitor.details.name\" | translate }}\r\n            </label>\r\n            <div class=\"p-col-12 p-md-10\">\r\n              <input\r\n                type=\"text\"\r\n                pInputText\r\n                id=\"name\"\r\n                [(ngModel)]=\"monitor.name\"\r\n                required\r\n                autofocus\r\n              />\r\n              <small class=\"p-error\" *ngIf=\"submitted && !monitor.name\"\r\n                >Name is required.</small\r\n              >\r\n            </div>\r\n          </div>\r\n          <div class=\"p-field p-grid\">\r\n            <label for=\"areas\" class=\"p-col-12 p-mb-2 p-md-2 p-mb-md-0\">\r\n              {{ \"monitor.details.area\" | translate }}\r\n            </label>\r\n            <div class=\"p-col-12 p-md-10\">\r\n              <p-multiSelect\r\n                id=\"areas\"\r\n                [options]=\"areas\"\r\n                [(ngModel)]=\"monitor.areas\"\r\n                appendTo=\"body\"\r\n                defaultLabel=\"{{ 'select-areas' | translate }}\"\r\n                [filter]=\"false\"\r\n              >\r\n                <ng-template let-areas pTemplate=\"selectedItems\">\r\n                  <div class=\"multiselect-values\">\r\n                    <span *ngFor=\"let area of areas\">\r\n                      {{ area | translate }}\r\n           